    "editorInfo.foreground": "#1D9BF0",
    "editorHint.foreground": "#8B98A5"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8B98A5"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1D9BF0"
      }
    },
    {
      "scope": [
        "constant",
        "constant.character.escape",
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#E8B339"
      }
    },
    {
      "scope": [
        "string",
        "string.template",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#41C995"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#E7E9EA"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#C2E7FF"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#9FB7D0"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#C7D1D9"
      }
    },
    {
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#8B98A5"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F4212E"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#C7D1D9",
    "property": "#C7D1D9",
    "namespace": "#9FB7D0",
    "enum": "#9FB7D0",
    "enumMember": "#E8B339",
    "parameter": "#E7E9EA",
    "typeParameter": "#9FB7D0",
    "function": "#C2E7FF",
    "method": "#C2E7FF",
    "string": "#41C995",
    "number": "#E8B339",
    "comment": "#8B98A5",
    "keyword": "#1D9BF0",
    "interface": "#9FB7D0",
    "class": "#9FB7D0"
  }
}